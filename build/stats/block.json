{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wsb-tailwindui/stats",
  "version": "0.1.0",
  "title": "TW Stats",
  "category": "wsb-tailwindui",
  "icon": "dashicons-align-full-width",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false,
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "textdomain": "wsb-tailwindui-stats",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "spacing": {
      "type": "object",
      "default": {
        "top": "0px",
        "left": "0px",
        "right": "0px",
        "bottom": "0px"
      }
    },
    "backgroundColor": {
      "type": "string"
    },
    "statBackgroundColor": {
      "type": "string"
    },
    "titleTextColor": {
      "type": "string"
    },
    "statTextColor": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "default": "Trusted by event hosts around the globe."
    },
    "years": {
      "type": "string",
      "default": "43"
    },
    "events": {
      "type": "string",
      "default": "100,000"
    },
    "speakers": {
      "type": "string",
      "default": "4,000+"
    }
  }
}