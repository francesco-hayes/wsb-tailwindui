{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wsb-tailwindui/hero-cta",
  "version": "0.1.0",
  "title": "TW Hero CTA",
  "category": "wsb-tailwindui",
  "icon": "dashicons-align-full-width",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "wsb-tailwindui-hero-cta",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "title": {
      "type": "string",
      "selector": "h1"
    },
    "paragraph": {
      "type": "string",
      "selector": "p"
    },
    "imageURL": {
      "type": "string",
      "selector": "img",
      "attribute": "src",
      "default": "https://images.unsplash.com/photo-1521737604893-d14cc237f11d?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2830&q=80&blend=111827&sat=-100&exp=15&blend-mode=multiply"
    },
    "imageID": {
      "type": "number",
      "default": null
    },
    "imageALT": {
      "type": "string",
      "selector": "img",
      "attribute": "alt"
    },
    "buttons": {
      "type": "array",
      "default": [
        {
          "url": "#",
          "text": "Get Started"
        },
        {
          "url": "#",
          "text": "Learn more"
        }
      ],
      "source": "query",
      "selector": "a",
      "query": {
        "url": {
          "type": "string",
          "source": "attribute",
          "attribute": "href"
        },
        "text": {
          "type": "string",
          "source": "text",
          "selector": "a"
        }
      }
    }
  }
}